<?xml version="1.0" encoding="ISO-8859-1"?>

<agents>
	<!-- A list of agents and their behavior classes -->
	
	<!-- #### REACTIVE AGENTS ### -->
	<agent name="reactive-rla">
		<set class-path="bin/"/>
		<set class-name="template.ReactiveTemplate"/>
		<set discount-factor=".85"/>
	</agent>
	
	<agent name="reactive-random">
		<set class-path="bin/"/>
		<set class-name="template.ReactiveTemplateRdm"/>
		<set discount-factor=".5"/>
	</agent>
	
	<agent name="reactive-deterministic">
		<set class-path="bin/"/>
		<set class-name="template.ReactiveTemplateRdm"/>
		<set discount-factor="1.0"/>
	</agent>
	
</agents>
